{
    "manifest_version": 2,
    "name": "Project Filo - Make your internet a little more faster.",
    "short_name": "Project Filo",
    "description": "Accelerate your internet experience even in 3G.",
    "version": "2.1.0",
    "icons": {
      "64": "assets/icon-128.png",
      "128": "assets/icon-256.png"
    },
    "browser_action": {
      "default_icon": "assets/icon-128.png",
      "default_title": "Project Filo",
      "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["index.js", "service.js"]
    },
    "permissions": [
      "activeTab",
      "tabs",
      "storage",
      "webRequest",
      "webRequestBlocking",
      "<all_urls>"
    ]
  }