{
  "manifest_version": 2,
  "name": "Project Filo - Alpha",
  "short_name": "Filo Alpha",
  "description": "Make your internet a little more faster.",
  "version": "0.0.1",
  "icons": {
    "64": "assets/icon-128.png",
    "128": "assets/icon-256.png"
  },
  "browser_action": {
    "default_icon": "assets/icon-128.png",
    "default_title": "Filo Alpha",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["update.bundle.js", "background.bundle.js"]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
