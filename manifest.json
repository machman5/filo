{
  "version": "0.0.3",
  "name": "Filo",
  "description": "Compression proxy helper for compy",
  "author": "sr229",
  "permissions": ["webRequest", "webRequestBlocking", "declarativeWebRequest", "proxy", "*://*/*"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "off.png",
    "default_title": "Data Compression Proxy: Disabled"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
